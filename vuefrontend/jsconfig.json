      "@router/*": ["router/*"],
      "@store/*": ["store/*"],
      "@stores/*": ["stores/*"],
      "@utils/*": ["utils/*"],
      "@types/*": ["types/*"],
      "@transactions/*": ["components/transactions/*"],
