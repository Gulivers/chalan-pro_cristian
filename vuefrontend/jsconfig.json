    "baseUrl": "src",
    "paths": {
      "@/*": ["*"],
      "@assets/*": ["assets/*"],
      "@assets/css/*": ["assets/css/*"],
      "@assets/img/*": ["assets/img/*"],
      "@assets/scss/*": ["assets/scss/*"],
      "@auth/*": ["auth/*"],
      "@components/*": ["components/*"],
      "@contracts/*": ["components/contracts/*"],
      "@buttons/*": ["components/buttons/*"], // Oliver Hernandez
      "@houses/*": ["components/houses/*"],
